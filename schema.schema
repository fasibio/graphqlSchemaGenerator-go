type query{
  registryUser(password: String, email: String): Boolean
  confirmUser(token: String)
  login(username: String, password: String): UserToken
  logout(refreshToken: String): Boolean
  verifyToken(accessToken: String)
  userInfo(accessToken: String)
}

type userToken{
  accessToken: String
  refreshToken: String
}

type user{
  email: String
  registryDate: String
  publicKey: [String]
}